        <servlet-class>com.se1625.controller.LoadApplyCVStudentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ApplyCVStudentServlet</servlet-name>
        <servlet-class>com.se1625.controller.ApplyCVStudentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CVStudentServlet</servlet-name>
        <servlet-class>com.se1625.controller.CVStudentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginGoogleServlet</servlet-name>
        <servlet-class>com.se1625.controller.LoginGoogleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginAccountsServlet</servlet-name>
        <servlet-class>com.se1625.controller.LoginAccountsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentDashboardServlet</servlet-name>
        <servlet-class>com.se1625.controller.StudentDashboardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchSaveJobServlet</servlet-name>
        <servlet-class>com.se1625.controller.SearchSaveJobServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeShowCompanyDetailServlet</servlet-name>
        <servlet-class>com.se1625.controller.HomeShowCompanyDetailServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchCompanyStudentHomeServlet</servlet-name>
        <servlet-class>com.se1625.controller.SearchCompanyStudentHomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowStudentHomeServlet</servlet-name>
        <servlet-class>com.se1625.controller.ShowStudentHomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateStudentProfileServlet</servlet-name>
        <servlet-class>com.se1625.controller.UpdateStudentProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentDeleteSaveJobServlet</servlet-name>
        <servlet-class>com.se1625.controller.StudentDeleteSaveJobServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShowStudentProfileServlet</servlet-name>
        <servlet-class>com.se1625.controller.ShowStudentProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutAccountServlet</servlet-name>
        <servlet-class>com.se1625.controller.LogoutAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StudentSaveJobServlet</servlet-name>
        <servlet-class>com.se1625.controller.StudentSaveJobServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ImportStudentExcelFileServlet</servlet-name>
        <url-pattern>/ImportStudentExcelFileServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExportEvalutionExcelFileServlet</servlet-name>
        <url-pattern>/ExportEvalutionExcelFileServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadApplyCVStudentServlet</servlet-name>
        <url-pattern>/LoadApplyCVStudentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
